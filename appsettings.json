{
  "ConnectionStrings": {
    "connString": "Server={{server}};Database={{database}};User Id={{username}};Password={{password}};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "jwtTokenConfig": {
    "secret": "1234567890123456788",
    "issuer": "https://OnlineStore",
    "audience": "https://OnlineStore",
    "accessTokenExpiration": 24,
    "refreshTokenExpiration": 24
  }
}